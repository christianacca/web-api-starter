{
  "AllowedHosts": "*",
  "Api": {
    "ConfigStoreFeatureFlags": {
      "Enabled": true
    },
    "ConfigStoreSections": ["Serilog"],
    "ConfigStoreUri": null,
    "Database": {
      "DataSource": null,
      "InitialCatalog": null,
      "Authentication": "ActiveDirectoryManagedIdentity",
      "UserID": null
    },
    "TokenProvider": {
      "Authority": null,
      "Audience": "api://default"
    },
    "DefaultAzureCredentials": {
      "ManagedIdentityClientId": null
    },
    "FunctionsAppToken": {
      "Audience": null
    },
    "FunctionsAppQueue": {
      "ServiceUri": null
    },
    "KeyVaultName": null,
    "KeyVaultReloadInterval": "00:30:00",
    "KeyVaultSections": [
      "CentralIdentity"
    ],
    "ReverseProxy": {
      "Routes": {
        "WorkflowWebhookFunctions": {
          "ClusterId": "FunctionsApp",
          "AuthorizationPolicy": "Anonymous",
          "CorsPolicy": "default",
          "Match": {
            "Path": "api/workflow/webhook",
            "Headers": [
              {
                "Name": "X-Correlation-Id",
                "Values": [ "InternalApi" ],
                "Mode": "Contains",
                "IsCaseSensitive": false
              }

            ]
          },
          "Transforms": [
            { "PathPattern": "/api/workflow/webhook" }
          ]
        },
        "Functions": {
          "ClusterId": "FunctionsApp",
          "AuthorizationPolicy": "default",
          "CorsPolicy": "default",
          "Match": {
            "Path": "{**catch-all}"
          }
        }
      },
      "Clusters": {
        "FunctionsApp": {
          "Destinations": {
            "Primary": {
              "Address": null
            }
          }
        }
      }
    }
  },
  "ApplicationInsights": {
    "AutoCollectActionArgs": false,
    "ConnectionString": null,
    "Enable": true,
    "SamplingPercentage": 100
  },
  "CentralIdentity": {
    "BaseUri": null,
    "Credentials": {
      "Username": "web-api-starter",
      "Password": null
    }
  },
  "EnvironmentInfo": {
    "EnvId": null,
    "InfraVersion": null
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Fatal"
    }
  },
  "Github": {
    "Owner": null,
    "Repo": null,
    "Branch": null,
    "WorkflowFile": null,
    "AppId": null,
    "InstallationId": 0,
    "PrivateKeyPem": null,
    "WebhookSecret": null
  }
}
