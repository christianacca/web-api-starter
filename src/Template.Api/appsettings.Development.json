{
  "Api": {
    "DefaultAzureCredentials": {
      "ExcludeManagedIdentityCredential": true
    },
    "TokenProvider": {
      "Authority": "https://mrisaas.oktapreview.com/oauth2/default"
    },
    "ReverseProxy": {
      "Clusters": {
        "FunctionsApp": {
          "Destinations": {
            "Primary": {
              "Address": "http://localhost:7071"
            }
          }
        }
      }
    }
  },
  "ApplicationInsights": {
    "AutoCollectActionArgs": true,
    "ConnectionString": "InstrumentationKey=whatever",
    "Enabled": true,
    "SamplingPercentage": 100,
    "DeveloperMode": true
  },
  "ConnectionStrings": {
    "AppDatabase": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=web-api-starter-template-db;Integrated Security=True;"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    }
  }
}
