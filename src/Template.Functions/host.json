{
  "version": "2.0",
  "logging": {
    "// logLevel: Host.Aggregator": "This is disabled to reduce log costs and can be enabled when deeper diagnostics are needed.",
    "logLevel": {
      "default": "Warning",
      "Host.Results": "Information",
      "Host.Startup": "Information",
      "Host.Triggers": "Information",
      "Function": "Information",
      "Octokit.Webhooks": "Error"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensions": {
    "queues": {
      "visibilityTimeout": "00:00:30",
      "maxDequeueCount": 5
    },
    "http": {
      "EnableChunkedRequestBinding": true
    }
  }
}